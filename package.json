{
    "name": "form-generator-plugin",
    "displayName": "Form Generator Plugin",
    "description": "Element UI可视化表单设计及代码生成器",
    "version": "1.0.4",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "author": {
        "name": "JakHuang"
    },
    "icon": "images/logo.png",
    "main": "./extension.js",
    "publisher": "jakHuang",
    "homepage": "https://github.com/JakHuang/form-generator-plugin/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/JakHuang/form-generator-plugin"
    },
    "activationEvents": [
        "onCommand:extension.openFormGenerator",
        "onCommand:test.file",
        "onView:shortcutResources"
    ],
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "shortcutTogether",
                    "title": "🔥",
                    "icon": "images/fire.svg"
                }
            ]
        },
        "views": {
            "shortcutTogether": [
                {
                    "id": "shortcutResources",
                    "name": "快捷方式-地址"
                },
                {
                    "id": "shortcutResources2",
                    "name": "快捷方式2"
                }
            ]
        },
        "configuration": {
            "type": "object",
            "title": "设计器配置",
            "properties": {
                "openFormGenerator.src": {
                    "type": "string",
                    "default": "http://localhost:3002/nuwa/demo",
                    "description": "设计器远程链接"
                }
            }
        },
        "commands": [
            {
                "command": "extension.openFormGenerator",
                "title": "🔥 Low Code"
            },
            {
                "command": "test.file",
                "title": "file"
            }
        ],
        "keybindings": [
            {
                "command": "extension.openFormGenerator",
                "key": "ctrl+f10",
                "mac": "cmd+f10",
                "when": "editorTextFocus"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "editorFocus",
                    "command": "extension.openFormGenerator",
                    "group": "navigation"
                }
            ],
            "explorer/context": [
                {
                    "command": "extension.openFormGenerator",
                    "group": "navigation"
                },
                {
                    "command": "test.file",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.0.0",
        "eslint": "^6.6.0",
        "glob": "^7.1.5",
        "mocha": "^6.2.2",
        "typescript": "^3.6.4",
        "vscode-test": "^1.2.2"
    },
    "dependencies": {
        "open": "^7.0.0"
    }
}
